{
  "actions" : {
    "build" : {
      "enabled" : true,
      "script" : "bundle install\njekyll build"
    },
    "clean" : {
      "enabled" : true,
      "script" : "jekyll clean"
    },
    "run" : {
      "enabled" : true,
      "script" : "jekyll serve --port 8090"
    }
  },
  "openLogOnRun" : "start",
  "persistent" : true
}
